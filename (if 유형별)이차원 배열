#include <stdio.h>

main() {
	
	int a, b, sum = 0;
	int num[3][3] = {
		{1,2,3},
		{4,5,6},
		{7,1,6}
	};
	for(a = 0;a <= 2;a++){
		for(b = 0;b <= 2;b++) {
			if(a>=b)
				sum += num[a][b];
		}
	}
	printf("%d", sum);
	
}
//sum = 0 + 1 + 5 + 6 = 12
//if(a <= b) : sum = 0 + 5 + 6 + 2 + 3 + 6 = 23
//if(a < b) : 0 + 2 + 3 + 6 = 11
//if(a > b) : 0 + 4 + 7 + 1 = 12
//if(a >= b) : 0 + 4 + 7 + 1 + 1 + 5 + 6 = 24
