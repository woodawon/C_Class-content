#include <stdio.h>

main() {
	
	int su[7] = {1, 1, 0, 0, 1, 0, 1};
	int a, cnt = 0, pa = 0;
	
	for(a = 0;a <= 6;a++) {
		if(su[a] == 1)
			cnt += 1;
	}
	if(cnt % 2 == 0)
		pa = 1;
	printf("%d\n", pa);
	
}
//0 1 2 3 4 5 6 
//1 1 0 0 1 0 1 
//
//a = 0, cnt = 1
//a = 1, cnt = 2
//a = 2, cnt = 2
//a = 3, cnt = 2
//a = 4, cnt = 3
//a = 5, cnt = 3
//a = 6, cnt = 4, pa = 1
